#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char s[1000];
    int count,key;
    scanf("%s",s);
    for(int i=0;i<10;i++){
        count=0;
        for(int j=0;j<s[i]!='\0';j++){
            key=s[j]-'0';
            if(key==i){
                count++;
            }
        }
        printf("%d ",count);
    }
    return 0;
}
