#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char s[100];
    int count;
    gets(s);
    for(int i=0;i<10;i++){
        count=0;
        for(int j=0;j<s[j]!='\0';j++)
            if(s[j]==i+'0')
                count++;
        printf("%d ",count);
    }
    return 0;
}
